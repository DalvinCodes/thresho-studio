{
  "name": "thresho-studio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "e2e": "playwright test -c e2e/playwright/config/playwright.config.ts",
    "e2e:smoke": "playwright test -c e2e/playwright/config/playwright.config.ts e2e/playwright/tests/smoke",
    "e2e:workflows": "playwright test -c e2e/playwright/config/playwright.config.ts e2e/playwright/tests/workflows",
    "e2e:real": "USE_REAL_PROVIDERS=true playwright test -c e2e/playwright/config/playwright.config.ts",
    "e2e:ui": "playwright test -c e2e/playwright/config/playwright.config.ts --ui",
    "e2e:debug": "playwright test -c e2e/playwright/config/playwright.config.ts --debug",
    "e2e:report": "playwright show-report playwright-report"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@sqlite.org/sqlite-wasm": "^3.51.2-build6",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "@xstate/react": "^6.0.0",
    "immer": "^11.1.3",
    "jotai": "^2.17.0",
    "jszip": "^3.10.1",
    "monaco-editor": "^0.55.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.18",
    "uuid": "^13.0.0",
    "xstate": "^5.26.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
